<div class="row">
	<label for="phpinfo-filter" class="form-label mt-2 mb-3">
		<input id="phpinfo-filter" type="text" class="form-control" placeholder="{{ lang('SEARCH_HERE') }}">
	</label>
</div>
<ul class="nav flex-column text-nowrap">
	<li class="nav-item">
		<a class="nav-link" href="#back-top">
			<svg class="bi me-1" width="16" height="16" fill="currentColor">
				<use xlink:href="{{ QI_ROOT_PATH }}style/assets/img/bootstrap-icons.svg#arrow-90deg-up"/>
			</svg>
			{{ lang('BACK_TOP') }}
		</a>
	</li>
	{% for info in phpinfo %}
		<li class="nav-item">
			<a class="nav-link" href="#{{ info.U_ANCHOR }}">
				<svg class="bi me-1" width="16" height="16" fill="currentColor">
					<use xlink:href="{{ QI_ROOT_PATH }}style/assets/img/bootstrap-icons.svg#bookmark"/>
				</svg>
				{{ info.TITLE }}
			</a>
		</li>
	{% endfor %}
</ul>
